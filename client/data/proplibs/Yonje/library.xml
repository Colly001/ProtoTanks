<library name="yonje">	
	<prop-group name="yonje">
		<prop name="Door">
			<mesh file="door.3DS">
				<texture name="North" diffuse-map="doorN.jpg" />
				<texture name="South" diffuse-map="doorS.jpg" />
			</mesh>
		</prop>
		<prop name="window">
			<mesh file="window.3DS">
				<texture name="North" diffuse-map="windowN.jpg" />
