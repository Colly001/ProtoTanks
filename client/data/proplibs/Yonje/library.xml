				<texture name="South" diffuse-map="endS.jpg" />
				<texture name="East" diffuse-map="endE.jpg" />
			</mesh>
		</prop>
		<prop name="corner">
			<mesh file="corner.3DS">
				<texture name="NorthWest" diffuse-map="cornerNW.jpg" />
